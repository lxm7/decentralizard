{
  "name": "decentralizard",
  "packageManager": "yarn@4.7.0",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && yarn build && yarn start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation yarn --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "rm -rf .next node_modules .yarn/cache && yarn install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "seed": "payload run ./scripts/seed-blog-posts.ts",
    "getCategories": "payload run ./scripts/getCategories.ts",
    "test": "jest --watch",
    "prepare": "husky install",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@faceless-ui/modal": "3.0.0",
    "@faceless-ui/scroll-info": "2.0.0",
    "@payloadcms/db-postgres": "3.72.0",
    "@payloadcms/live-preview-react": "3.72.0",
    "@payloadcms/next": "3.72.0",
    "@payloadcms/payload-cloud": "3.72.0",
    "@payloadcms/plugin-form-builder": "3.72.0",
    "@payloadcms/plugin-nested-docs": "3.72.0",
    "@payloadcms/plugin-redirects": "3.72.0",
    "@payloadcms/plugin-search": "3.72.0",
    "@payloadcms/plugin-seo": "3.72.0",
    "@payloadcms/richtext-lexical": "3.72.0",
    "@payloadcms/ui": "3.72.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@supabase/supabase-js": "^2.49.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "d3": "^7.9.0",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "15.5.9",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.12",
    "payload": "3.72.0",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.3.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7",
    "@types/escape-html": "^1.0.2",
    "@types/eslint-plugin-tailwindcss": "^3",
    "@types/gtag.js": "^0.0.20",
    "@types/jest": "^29.5.14",
    "@types/node": "22.5.4",
    "@types/nodemailer": "^6",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "^10.4.19",
    "baseline-browser-mapping": "^2.9.14",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.38",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "resolutions": {
    "tar": "^7.5.2",
    "esbuild": "^0.25.12"
  }
}
